{
"name": "YouTubeTracker",
"description": "Track time spent on YouTube and make user aware of this",
"version": "1.0"

"manifest_version": 2,

"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
},

"permissions": [
    "tabs",
    "storage",
    "declarativeContent"
],

"background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}
