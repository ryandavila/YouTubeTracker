{
  "name": "YouTubeTracker",
  "version": "1.0",
  "description": "Track time spent on YouTube and make user aware of this",
  "background": {
      "scripts": ["background.js", "popup.js"],
      "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs","storage","declarativeContent"],
  "manifest_version": 2
}
